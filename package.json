{
  "name": "selek",
  "version": "2.0.0",
  "description": "Selek - Systematic Multi-Agent AI Platform with Local LLM Integration",
  "main": "dist/index.js",
  "bin": {
    "selek": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "cli": "tsx src/cli.ts",
    "chat": "tsx src/lib/chat/chat.ts",
    "mcp:server": "tsx src/mcp/mcp-server.ts",
    "dev": "ts-node-dev --respawn --loader ts-node/esm src/server.ts",
    "tui": "tsx src/tui/multiagent-tui.tsx",
    "tui:classic": "tsx src/tui.tsx",
    "tui:dev": "tsx watch src/tui/multiagent-tui.tsx",
    "start": "node dist/cli.js",
    "start:server": "node dist/server.js",
    "start:tui": "node dist/tui.js",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "agent",
    "multi-agent",
    "ollama",
    "code-assistant",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.4.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphology": "^0.26.0",
    "graphology-types": "^0.24.8",
    "gray-matter": "^4.0.3",
    "ink": "^3.2.0",
    "ink-select-input": "^4.2.1",
    "inquirer": "^8.2.5",
    "openai": "^6.7.0",
    "ora": "^5.4.1",
    "react": "^17.0.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.26",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
